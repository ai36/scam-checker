{
    "version": 2,
    "builds": [
      {
        "src": "api/check-url.js",
        "use": "@vercel/node"
      },
      {
        "src": "index.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/check-url",
        "methods": ["POST"],
        "dest": "/api/check-url"
      },
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ]
  }