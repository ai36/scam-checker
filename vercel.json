  {
    "version": 2,
    "builds": [
      {
        "src": "api/check-url.js",
        "use": "@vercel/node"
      },
      {
        "src": "index.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/check-url",
        "dest": "api/check-url.js"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  